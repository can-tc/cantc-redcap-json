[
  {
    "form_name": "Patient Information",
    "event_name": "enrolment",
    "sections": [
      {
        "section_name": "demographics",
        "fields": [
          {
            "variable_name": "age_at_diagnosis",
            "input_type": "text",
            "data_type": "float",
            "optionality": "mandatory"
          },
          {
            "variable_name": "sex",
            "input_type": "radio",
            "data_type": "string",
            "optionality": "mandatory",
            "values": [
              "0",
              "1",
              "na"
            ]
          },
          {
            "variable_name": "postalcode_avail",
            "input_type": "radio",
            "data_type": "string",
            "optionality": "mandatory",
            "values": [
              "yes",
              "no"
            ]
          },
          {
            "variable_name": "postal_code",
            "input_type": "text",
            "data_type": "string",
            "optionality": "mandatory",
            "ancestor": [
              "postalcode_avail"
            ],
            "condition": "postalcode_avail set to yes"
          },
          {
            "variable_name": "iq_diagnosis",
            "input_type": "text",
            "data_type": "integer",
            "optionality": "optional",
            "ancestor": [
              "postalcode_avail"
            ],
            "condition": "no selected above"
          },
          {
            "variable_name": "sac_code_diagnosis",
            "input_type": "text",
            "data_type": "integer",
            "optionality": "optional",
            "ancestor": [
              "postalcode_avail"
            ],
            "condition": "no selected above"
          },
          {
            "variable_name": "currnent_iq",
            "input_type": "text",
            "data_type": "integer",
            "optionality": "optional",
            "ancestor": [
              "postalcode_avail"
            ],
            "condition": "no selected above"
          },
          {
            "variable_name": "sa_code_current",
            "input_type": "text",
            "data_type": "integer",
            "optionality": "optional",
            "ancestor": [
              "postalcode_avail"
            ],
            "condition": "no selected above"
          },
          {
            "variable_name": "distance_tertiary",
            "input_type": "text",
            "data_type": "float",
            "optionality": "optional",
            "ancestor": [
              "postalcode_avail"
            ],
            "condition": "no selected above"
          }
        ]
      },
      {
        "section_name": "Location of Surgery",
        "fields": [
          {
            "variable_name": "country_s",
            "input_type": "radio",
            "data_type": "string",
            "optionality": "mandatory",
            "values": [
              "Canada",
              "Other"
            ]
          },
          {
            "variable_name": "city_surgery",
            "input_type": "select",
            "data_type": "string",
            "optionality": "optional",
            "ancestor": [
              "country_s"
            ],
            "condition": "country_s set to canada"
          },
          {
            "variable_name": "hospital_fs",
            "input_type": "text",
            "data_type": "string",
            "optionality": "optional",
            "ancestor": [
              "city_surgery",
              "country_s"
            ],
            "condition": "city of surgery is not null"
          },
          {
            "variable_name": "type_facility",
            "input_type": "radio",
            "date_type": "string",
            "optionality": "optional",
            "ancestor": [
              "city_surgery",
              "country_s"
            ],
            "condition": "hospital_fs is not null"
          }
        ]
      },
      {
        "section_name": "Family History of Thyroid Cancer",
        "fields": [
          {
            "variable_name": "family_history",
            "input_type": "radio",
            "data_type": "string",
            "optionality": "optional",
            "values": [
              "Yes",
              "no",
              "n/a"
            ]
          }
        ]
      },
      {
        "section_name": "Prior Radiation Exposure",
        "fields": [
          {
            "variable_name": "radiation_exposure",
            "input_type": "radio",
            "data_type": "string",
            "optionality": "optional",
            "values": [
              "Yes",
              "No",
              "Unknown",
              "n/a"
            ]
          },
          {
            "variable_name": "radiation_exposure_details",
            "input_type": "radio",
            "data_type": "string",
            "optionality": "optional",
            "values": [
              "Therapeutic",
              "Non-diagnostic",
              "Unknown"
            ]
          }
        ]
      }
    ]
  },
  {
    "form_name": "Surgical Pathology",
    "event_name": "enrolment",
    "sections": [
      {
        "section_name": "Surgery Information",
        "fields": [
          {
            "variable_name": "date_of_diagnosis",
            "input_type": "text",
            "data_type": "date",
            "optionality": "mandatory"
          },
          {
            "variable_name": "surgery_type",
            "input_type": "checkbox",
            "data_type": "string",
            "optionality": "mandatory",
            "values": [
              "Thyroid",
              "Neck Dissection"
            ]
          },
          {
            "variable_name": "ts_type",
            "input_type": "radio",
            "data_type": "string",
            "optionality": "optional",
            "values": [
              "total",
              "hemi",
              "lobectomy"
            ],
            "ancestor": [
              "surgery_type"
            ],
            "condition": "thyroid selected"
          },
          {
            "variable_name": "hemi_side",
            "input_type": "radio",
            "data_type": "string",
            "optionality": "mandatory",
            "values": [
              "Left",
              "Right"
            ],
            "ancestor": [
              "ts_type",
              "surgery_type"
            ],
            "condition": "ts_type set to hemi"
          },
          {
            "variable_name": "completion_date",
            "input_type": "date",
            "data_type": "date",
            "optionality": "mandatory",
            "ancestor": [
              "ts_type",
              "surgery_type"
            ],
            "condition": "ts_type set to hemi"
          },
          {
            "variable_name": "lobectomy_side",
            "input_type": "radio",
            "data_type": "string",
            "optionality": "mandatory",
            "values": [
              "Left",
              "Right"
            ],
            "ancestor": [
              "ts_type",
              "surgery_type"
            ],
            "condition": "ts_type set to lobectomy"
          },
          {
            "variable_name": "lobectomy_completion_date",
            "input_type": "date",
            "data_type": "date",
            "optionality": "optional",
            "ancestor": [
              "ts_type",
              "surgery_type"
            ],
            "condition": "ts_type set to hemi"
          },
          {
            "variable_name": "neck_dissection_side",
            "input_type": "checkbox",
            "data_type": "string",
            "optionality": "mandatory",
            "values": [
              "Central",
              "Lateral",
              "Mediastinal"
            ],
            "ancestor": [
              "surgery_type",
              "ts_type"
            ],
            "condition": "neck dissection selected"
          },
          {
            "variable_name": "cnd_location",
            "input_type": "select",
            "data_type": "string",
            "optionality": "mandatory",
            "values": [
              "Left",
              "Right",
              "Left/Right",
              "n/a"
            ],
            "ancestor": [
              "neck_dissection_side",
              "ts_type",
              "surgery_type"
            ],
            "condition": "Central selected"
          },
          {
            "variable_name": "lnd_location",
            "input_type": "select",
            "data_type": "string",
            "optionality": "mandatory",
            "values": [
              "Left",
              "Right",
              "Left/Right",
              "na"
            ],
            "ancestor": [
              "neck_dissection_side",
              "ts_type",
              "surgery_type"
            ],
            "condition": "Lateral selected"
          }
        ]
      },
      {
        "section_name": "Histology",
        "fields": [
          {
            "variable_name": "nodule_distribution",
            "input_type": "select",
            "data_type": "string",
            "optionality": "mandatory",
            "values": [
              "Single",
              "Multifocal",
              "Unknown"
            ]
          },
          {
            "variable_name": "is_multiple_histology",
            "input_type": "radio",
            "data_type": "string",
            "optionality": "mandatory",
            "values": [
              "no",
              "yes"
            ]
          }
        ]
      },
      {
        "section_name": "Dominant Tumour Histology",
        "fields": [
          {
            "variable_name": "primary_histology_type",
            "input_type": "select",
            "data_type": "string",
            "optionality": "mandatory",
            "values": [
              "Papillary Carcinoma",
              "Follicular Carcinoma",
              "Poorly Differentiated",
              "Anaplastic",
              "Medullary",
              "Carcinoma Type cannot be determined"
            ]
          },
          {
            "variable_name": "papillary_variant",
            "input_type": "select",
            "data_type": "string",
            "optionality": "optional",
            "values": [
              "Classical",
              "Follicular (encapsulated)",
              "Follicular (infiltractive)",
              "Follicular (not specified)",
              "Oncocytic (Hurthle)",
              "Tall Cell",
              "Cribriform-Morular",
              "Diffuse Sclerosing",
              "NIFTP (non-invasive follicular variant of papillary)"
            ],
            "ancestor": [
              "primary_histology_type"
            ],
            "condition": "Papillary Carcinoma selected"
          },
          {
            "variable_name": "follicular_variant",
            "input_type": "select",
            "data_type": "string",
            "optionality": "optional",
            "values": [
              "Oncocytic (Hurthle)",
              "Other"
            ],
            "ancestor": [
              "primary_histology_type"
            ],
            "condition": "Follicular Carcinoma selected"
          },
          {
            "variable_name": "poorly_diff_variant",
            "input_type": "select",
            "data_type": "string",
            "optionality": "optional",
            "values": [
              "None",
              "Insular"
            ],
            "ancestor": [
              "primary_histology_type"
            ],
            "condition": "Poorly differentiated selected"
          },
          {
            "variable_name": "tumour_size",
            "input_type": "text",
            "data_type": "float",
            "optionality": "mandatory"
          },
          {
            "variable_name": "positive_margins",
            "input_type": "select",
            "data_type": "string",
            "optionality": "mandatory",
            "values": [
              "yes",
              "no",
              "cannot be assessed"
            ]
          },
          {
            "variable_name": "angio_invasion",
            "input_type": "select",
            "data_type": "string",
            "optionality": "mandatory",
            "values": [
              "yes",
              "no",
              "cannot be assessed"
            ]
          },
          {
            "variable_name": "angio_invasion_details",
            "input_type": "select",
            "data_type": "string",
            "optionality": "mandatory",
            "values": [
              "More than 4 vessels",
              "n/a"
            ],
            "ancestor": [
              "angio_invasion"
            ],
            "condition": "yes selected"
          },
          {
            "variable_name": "lmyphatic_invasion",
            "input_type": "select",
            "data_type": "string",
            "optionality": "mandatory",
            "values": [
              "yes",
              "no",
              "cannot be assessed"
            ]
          },
          {
            "variable_name": "perineural_invasion",
            "input_type": "select",
            "data_type": "string",
            "optionality": "mandatory",
            "values": [
              "yes",
              "no",
              "cannot be assessed"
            ]
          },
          {
            "variable_name": "extrathyroidal_extension",
            "input_type": "select",
            "data_type": "string",
            "optionality": "mandatory",
            "values": [
              "yes",
              "no",
              "cannot be assessed"
            ]
          },
          {
            "variable_name": "ete_details",
            "input_type": "radio",
            "data_type": "string",
            "optionality": "optional",
            "values": [
              "Minimal",
              "Extensive",
              "na"
            ],
            "ancestor": [
              "extrathyroidal_extension"
            ],
            "condition": "yes selected"
          },
          {
            "variable_name": "lymph_vascular_invasion",
            "input_type": "select",
            "data_type": "string",
            "optionality": "optional",
            "values": [
              "Yes",
              "No",
              "Cannot be assessed"
            ]
          },
          {
            "variable_name": "lvi_details",
            "input_type": "radio",
            "data_type": "string",
            "optionality": "optional",
            "values": [
              "More than 4 vessels",
              "n/a"
            ],
            "ancestor": [
              "lymph_vascular_invasion"
            ],
            "condition": "lymph_vascular_invasion selected as yes"
          }
        ]
      },
      {
        "section_name": "Secondary Tumour Histology",
        "fields": [
          {
            "variable_name": "secondary_histology_type",
            "input_type": "select",
            "data_type": "string",
            "optionality": "mandatory",
            "values": [
              "Papillary Carcinoma",
              "Follicular Carcinoma",
              "Poorly Differentiated",
              "Anaplastic",
              "Medullary",
              "Carcinoma Type cannot be determined"
            ],
            "ancestor": [
              "is_multiple_histology"
            ],
            "condition": "is_multiple_histology set to yes"
          },
          {
            "variable_name": "st_papillary_variant",
            "input_type": "select",
            "data_type": "string",
            "optionality": "optional",
            "values": [
              "Classical",
              "Follicular (encapsulated)",
              "Follicular (infiltractive)",
              "Follicular (not specified)",
              "Oncocytic (Hurthle)",
              "Tall Cell",
              "Cribriform-Morular",
              "Diffuse Sclerosing",
              "NIFTP (non-invasive follicular variant of papillary)"
            ],
            "ancestor": [
              "secondary_histology_type",
              "is_multiple_histology"
            ],
            "condition": "Papillary Carcinoma selected"
          },
          {
            "variable_name": "st_follicular_variant",
            "input_type": "radio",
            "data_type": "string",
            "optionality": "optional",
            "values": [
              "Oncocytic (Hurthle)",
              "Other"
            ],
            "ancestor": [
              "secondary_histology_type",
              "is_multiple_histology"
            ],
            "condition": "Follicular Carcinoma selected"
          },
          {
            "variable_name": "st_poorly_diff_variant",
            "input_type": "radio",
            "data_type": "string",
            "optionality": "optional",
            "values": [
              "None",
              "Insular"
            ],
            "ancestor": [
              "secondary_histology_type",
              "is_multiple_histology"
            ],
            "condition": "Poorly differentiated selected"
          },
          {
            "variable_name": "st_tumour_size",
            "input_type": "text",
            "data_type": "float",
            "optionality": "mandatory"
          },
          {
            "variable_name": "st_positive_margins",
            "input_type": "select",
            "data_type": "string",
            "optionality": "mandatory",
            "values": [
              "yes",
              "no",
              "cannot be assessed"
            ],
            "ancestor": [
              "is_multiple_histology"
            ],
            "condition": "is_multiple_histology set to yes"
          },
          {
            "variable_name": "st_angio_invasion",
            "input_type": "select",
            "data_type": "string",
            "optionality": "mandatory",
            "values": [
              "yes",
              "no",
              "cannot be assessed"
            ],
            "ancestor": [
              "is_multiple_histology"
            ],
            "condition": "is_multiple_histology set to yes"
          },
          {
            "variable_name": "st_angio_invasion_details",
            "input_type": "select",
            "data_type": "string",
            "optionality": "mandatory",
            "values": [
              "More than 4 vessels",
              "na"
            ],
            "ancestor": [
              "st_angio_invasion",
              "is_multiple_histology"
            ],
            "condition": "yes selected"
          },
          {
            "variable_name": "st_lmyphatic_invasion",
            "input_type": "select",
            "data_type": "string",
            "optionality": "mandatory",
            "values": [
              "yes",
              "no",
              "cannot be assessed"
            ],
            "ancestor": [
              "is_multiple_histology"
            ],
            "condition": "is_multiple_histology set to yes"
          },
          {
            "variable_name": "st_perineural_invasion",
            "input_type": "select",
            "data_type": "string",
            "optionality": "mandatory",
            "values": [
              "yes",
              "no",
              "cannot be assessed"
            ],
            "ancestor": [
              "is_multiple_histology"
            ],
            "condition": "is_multiple_histology set to yes"
          },
          {
            "variable_name": "st_extrathy_extension",
            "input_type": "select",
            "data_type": "string",
            "optionality": "mandatory",
            "values": [
              "yes",
              "no",
              "cannot be assessed"
            ],
            "ancestor": [
              "is_multiple_histology"
            ],
            "condition": "is_multiple_histology set to yes"
          },
          {
            "variable_name": "st_ete_details",
            "input_type": "radio",
            "data_type": "string",
            "optionality": "optional",
            "values": [
              "Minimal",
              "Extensive",
              "na"
            ],
            "ancestor": [
              "st_extrathyroidal_extension",
              "is_multiple_histology"
            ],
            "condition": "yes selected"
          },
          {
            "variable_name": "st_lymph_vascular_invasion",
            "input_type": "select",
            "data_type": "string",
            "optionality": "optional",
            "values": [
              "Yes",
              "No",
              "Cannot be assessed"
            ],
            "ancestor": [
              "is_multiple_histology"
            ],
            "condition": "is_multiple_histology set to yes"
          },
          {
            "variable_name": "st_lvi_details",
            "input_type": "radio",
            "data_type": "string",
            "optionality": "optional",
            "values": [
              "More than 4 vessels",
              "n/a"
            ],
            "ancestor": [
              "st_lymph_vascular_invasion"
            ],
            "condition": "yes selected"
          }
        ]
      },
      {
        "section_name": "Lymph Nodal Involvement",
        "fields": [
          {
            "variable_name": "total_lymph_nodes",
            "input_type": "text",
            "data_type": "integer",
            "optionality": "mandatory"
          },
          {
            "variable_name": "ln_na",
            "input_type": "checkbox",
            "data_type": "text",
            "optionality": "optional",
            "ancestor": [
              "total_lymph_nodes"
            ],
            "condition": "total_lymph_nodes >= 1"
          },
          {
            "variable_name": "positive_lymph_nodes",
            "input_type": "text",
            "data_type": "integer",
            "optionality": "mandatory",
            "ancestor": [
              "ln_na",
              "total_lymph_nodes"
            ],
            "condition": "ln_na not checked and total_lmph_nodes > 0"
          },
          {
            "variable_name": "location_lymph_nodes",
            "input_type": "select",
            "data_type": "text",
            "optionality": "mandatory",
            "values": [
              "N1a(central)",
              "N1b(lateral and/or mediastinal",
              "N1a/N1b(Central/lateral/mediastinal",
              "n/a"
            ],
            "ancestor": [
              "ln_na",
              "total_lymph_nodes",
              "positive_lymph_nodes"
            ],
            "condition": "positive_lymph_nodes >= 1"
          },
          {
            "variable_name": "extranodal_extension",
            "input_type": "select",
            "data_type": "string",
            "optionality": "mandatory",
            "values": [
              "yes",
              "no",
              "cannot be assessed"
            ],
            "ancestor": [
              "ln_na",
              "total_lymph_nodes",
              "positive_lymph_nodes"
            ],
            "condition": "positive_lymph_nodes >= 1"
          },
          {
            "variable_name": "size_ln",
            "input_type": "text",
            "data_type": "string",
            "optionality": "optional",
            "ancestor": "total_lymph_nodes",
            "condition": "total_lymph_nodes >= 1"
          }
        ]
      }
    ]
  },
  {
    "form_name": "Follow-up Record",
    "event_name": "Follow up",
    "longitudinal": true,
    "sections": [
      {
        "section_name": "none",
        "fields": [
          {
            "variable_name": "date_followup",
            "input_tupe": "date",
            "data_type": "date",
            "optionality": "mandatory"
          },
          {
            "variable_name": "lost_followup",
            "input_tupe": "radio",
            "data_type": "string",
            "optionality": "mandatory"
          },
          {
            "variable_name": "disease_status",
            "input_tupe": "select",
            "data_type": "string",
            "optionality": "mandatory",
            "values": [
              "Free of disease",
              "Recurrence",
              "Persistent disease",
              "Unknown"
            ]
          }
        ]
      },
      {
        "section_name": "Recurrence details",
        "fields": [
          {
            "variable_name": "confirmed_recurrence",
            "input_type": "radio",
            "data_type": "string",
            "optionality": "optional",
            "values": [
              "yes",
              "no",
              "n/a"
            ],
            "ancestor": [
              "disease_status"
            ],
            "condition": "Recurrence selected"
          },
          {
            "variable_name": "confirmation_method",
            "input_type": "checkbox",
            "data_type": "string",
            "optionality": "optional",
            "values": [
              "Pathologically",
              "RAI",
              "n/a"
            ],
            "ancestor": [
              "confirmed_recurrence",
              "disease_status"
            ],
            "condition": "Yes selected for confirmed recurrence"
          },
          {
            "variable_name": "not_confirmed_recurrence",
            "input_type": "checkbox",
            "data_type": "string",
            "optionality": "optional",
            "values": [
              "Pathological fracture",
              "Biochemical (Tg or Anti-TgAB)",
              "Imaging"
            ],
            "ancestor": [
              "confirmed_recurrence",
              "disease_status"
            ],
            "condition": "No selected for confirmed recurrence"
          }
        ]
      },
      {
        "section_name": "Persistent disease details",
        "fields": [
          {
            "variable_name": "persistent_disease",
            "input_type": "radio",
            "data_type": "string",
            "optionality": "mandatory",
            "values": [
              "Stable",
              "Improving",
              "Progressing"
            ],
            "ancestor": [
              "disease_status"
            ],
            "condtition": "Persistent disease selected"
          },
          {
            "variable_name": "disease_location",
            "input_type": "checkbox",
            "data_type": "string",
            "optionality": "mandatory",
            "values": [
              "Neck-Local (Thyroid Bed)",
              "Neck-Regional (Lateral Neck)",
              "Neck-not specified",
              "Mediastinal",
              "Lung",
              "Liver",
              "Brain",
              "Bone",
              "Muscle",
              "Biochemical Evidence of Disease",
              "Other"
            ],
            "ancestor": [
